USE DataLakeVentas
SELECT COUNT(*)as ventas,ciudad as ciudad_destino,SUM(cm.valor_recaudo)as valor_recaudo 
FROM plataforma_carga_masiva
	JOIN plataforma_ciudades_aveonline 
	on 
	cm.id_ciudad=c.id_ciudad
where proveedor_dropshipping = 7890 and estado_transportadora != 'Anulada' or estado_transportadora != 'NA'
group by ciudad 
order by ciudad 

go 

USE DataLakeVentas
SELECT COUNT(*)as ventas,id_tienda_generado as id_tienda,nombre_remitente as nombre_tienda 
FROM plataforma_carga_masiva 
where proveedor_dropshipping = 7890 and estado_transportadora != 'Anulada' or estado_transportadora != 'NA'
group by id_tienda_generado,nombre_remitente
order by ventas
